ets"], "authors": ["sloria"], "releases": [{"url": "https://codeload.github.com/sloria/sublime-nose-snippets/zip/master", "platforms": ["*"], "sublime_text": "*", "date": "2013-07-07 03:33:25", "version": "2013.07.07.03.33.25"}], "homepage": "https://github.com/sloria/sublime-nose-snippets", "readme": "https://raw.githubusercontent.com/sloria/sublime-nose-snippets/master/README.md", "description": "Sublime Text snippets for Python nose testing", "donate": null, "issues": "https://github.com/sloria/sublime-nose-snippets/issues", "name": "Python Nose Testing Snippets"}, {"previous_names": ["GotoLastEdit"], "buy": null, "labels": [], "authors": ["khrizt"], "releases": [{"url": "https://codeload.github.com/khrizt/GotoLastEdit/zip/2.0.1", "platforms": ["*"], "sublime_text": "*", "date": "2014-04-07 15:09:45", "version": "2.0.1"}], "homepage": "https://github.com/khrizt/GotoLastEdit", "readme": "https://raw.githubusercontent.com/khrizt/GotoLastEdit/master/README.md", "description": "Sublime plugin to go to last edit position", "donate": null, "issues": "https://github.com/khrizt/GotoLastEdit/issues", "name": "GoToLastEdit"}, {"previous_names": [], "buy": null, "labels": ["file navigation"], "authors": ["glutanimate"], "releases": [{"url": "https://codeload.github.com/glutanimate/sublime-open-in-nautilus/zip/1.0.0", "platforms": ["linux"], "sublime_text": ">=3000", "date": "2014-09-06 09:55:08", "version": "1.0.0"}], "homepage": "https://github.com/glutanimate/sublime-open-in-nautilus", "readme": "https://raw.githubusercontent.com/glutanimate/sublime-open-in-nautilus/master/README.md", "description": "Sublime Text plugin that opens files in Nautilus (default file manager for GNOME and Unity)", "donate": null, "issues": "https://github.com/glutanimate/sublime-open-in-nautilus/issues", "name": "Open in Nautilus"}, {"previous_names": [], "buy": null, "labels": ["fn", "language syntax"], "authors": ["jonnyarnold"], "releases": [{"url": "https://codeload.github.com/jonnyarnold/